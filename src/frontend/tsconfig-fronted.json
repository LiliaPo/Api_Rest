{
  "compilerOptions": {
    "target": "es5",
    "module": "ES2020",
    "moduleResolution": "Node",
    "outDir": "../../public/src",
    "includes": "../frondend",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}